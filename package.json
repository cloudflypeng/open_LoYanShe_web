{
  "name": "@nuxt-awesome/theme",
  "version": "2.0.12",
  "description": "a Nuxt 3 template and boilerplate with a lot of useful features. Nuxt 3 + Tailwindcss + Nuxt Layer",
  "author": "viandwi24",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/viandwi24/nuxt3-awesome-starter"
  },
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "modules",
    "pages",
    "public",
    "stores",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tailwind.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "nuxt build ./app",
    "dev": "nuxt dev ./app --no-clear",
    "generate": "nuxt generate ./app",
    "preview": "nuxt preview ./app",
    "postinstall": "nuxt prepare ./app",
    "dev:starter": "nuxt dev ./examples/starter",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/devtools": "^1.7.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@pinia/nuxt": "^0.5.5",
    "@vueuse/core": "^10.11.1",
    "@vueuse/nuxt": "^10.11.1",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-icon": "^0.6.10",
    "pinia": "^2.3.1",
    "sass": "^1.83.4",
    "stringify": "^5.2.0",
    "tailwindcss": "^3.4.17",
    "unplugin-vue-components": "^0.25.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "consola": "^3.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "nuxt": "^3.15.2",
    "prettier": "^3.4.2",
    "release-it": "^16.3.0",
    "serve": "^14.2.4",
    "typescript": "^5.7.3",
    "vue-tsc": "1.8.22"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
